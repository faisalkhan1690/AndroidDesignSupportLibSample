    <style name="AppThemeOne" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primary_one</item>
        <item name="colorPrimaryDark">@color/primary_dark_one</item>
        <item name="colorAccent">@color/accent_one</item>
    </style>

    <style name="AppThemeTwo" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
